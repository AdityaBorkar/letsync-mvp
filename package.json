{
  "author": "Aditya Borkar <hello@adityab.tech>",
  "bin": "./dist/cli/index.cjs",
  "dependencies": {
    "@aws-sdk/client-iot-data-plane": "^3.965.0",
    "arktype": "^2.1.29",
    "commander": "^14.0.2",
    "ora": "^9.0.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.8",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "uuid": "^13.0.0",
    "ws": "^8.19.0"
  },
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.5",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "knip": "^5.80.2",
    "next": "^16.1.1",
    "typescript": "^5.9.3",
    "zshy": "^0.7.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.cts"
    },
    "./drizzle": {
      "import": "./dist/(orm)/drizzle/index.js",
      "require": "./dist/(orm)/drizzle/index.cjs",
      "types": "./dist/(orm)/drizzle/index.d.cts"
    },
    "./next": {
      "import": "./dist/(adapters)/next/index.js",
      "require": "./dist/(adapters)/next/index.cjs",
      "types": "./dist/(adapters)/next/index.d.cts"
    },
    "./react": {
      "import": "./dist/(adapters)/react/index.js",
      "require": "./dist/(adapters)/react/index.cjs",
      "types": "./dist/(adapters)/react/index.d.cts"
    },
    "./web-server": {
      "import": "./dist/(adapters)/web-server/index.js",
      "require": "./dist/(adapters)/web-server/index.cjs",
      "types": "./dist/(adapters)/web-server/index.d.cts"
    },
    "./ws": {
      "import": "./dist/(adapters)/websockets/index.js",
      "require": "./dist/(adapters)/websockets/index.cjs",
      "types": "./dist/(adapters)/websockets/index.d.cts"
    },
    "./ws-server-patch": {
      "import": "./dist/(adapters)/ws-server-patch/index.js",
      "require": "./dist/(adapters)/ws-server-patch/index.cjs",
      "types": "./dist/(adapters)/ws-server-patch/index.d.cts"
    }
  },
  "files": ["dist", "LICENSE.md", "package.json", "README.md"],
  "keywords": ["letsync"],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "name": "letsync",
  "peerDependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "drizzle-kit": "^1.0.0-beta.9-e89174b",
    "drizzle-orm": "^1.0.0-beta.9-e89174b",
    "express": "^5.2.1"
  },
  "private": false,
  "publishConfig": {
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "zshy && biome check --fix ./package.json",
    "check:code": "biome check . --fix --diagnostic-level=error",
    "check:types": "tsc --noEmit",
    "check:unused": "knip",
    "dev": "bun run build && bun link",
    "format": "biome check . --fix --diagnostic-level=error",
    "publish": "npm publish --access public"
  },
  "trustedDependencies": ["oxc-resolver", "sharp"],
  "type": "module",
  "types": "./dist/index.d.cts",
  "version": "0.0.3",
  "zshy": {
    "bin": "./src/cli/index.ts",
    "exports": {
      ".": "./src/index.ts",
      "./drizzle": "./src/(orm)/drizzle/index.ts",
      "./next": "./src/(adapters)/next/index.ts",
      "./react": "./src/(adapters)/react/index.ts",
      "./web-server": "./src/(adapters)/web-server/index.ts",
      "./ws": "./src/(adapters)/websockets/index.ts",
      "./ws-server-patch": "./src/(adapters)/ws-server-patch/index.ts"
    }
  }
}

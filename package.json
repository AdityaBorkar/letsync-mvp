{
	"bin": "./dist/cli/index.cjs",
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.2",
		"arktype": "^2.1.20",
		"commander": "^14.0.0",
		"jsonwebtoken": "^9.0.2",
		"lodash.merge": "^4.6.2",
		"ora": "^8.2.0",
		"postgres": "^3.4.7",
		"ws": "^8.18.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.2",
		"@types/bun": "^1.2.18",
		"@types/express": "^5.0.3",
		"@types/lodash.merge": "^4.6.9",
		"@types/node": "^24.0.14",
		"@types/ws": "^8.18.1",
		"knip": "^5.62.0",
		"typescript": "^5.8.3",
		"zshy": "^0.2.5"
	},
	"exports": {
		"./blunt": {
			"types": "./dist/(frameworks)/blunt/index.d.cts",
			"import": "./dist/(frameworks)/blunt/index.js",
			"require": "./dist/(frameworks)/blunt/index.cjs"
		},
		"./device-client": {
			"types": "./dist/device-client/index.d.cts",
			"import": "./dist/device-client/index.js",
			"require": "./dist/device-client/index.cjs"
		},
		"./mutations": {
			"types": "./dist/mutations/index.d.cts",
			"import": "./dist/mutations/index.js",
			"require": "./dist/mutations/index.cjs"
		},
		"./next": {
			"types": "./dist/(frameworks)/next/index.d.cts",
			"import": "./dist/(frameworks)/next/index.js",
			"require": "./dist/(frameworks)/next/index.cjs"
		},
		"./react": {
			"types": "./dist/(frameworks)/react/index.d.cts",
			"import": "./dist/(frameworks)/react/index.js",
			"require": "./dist/(frameworks)/react/index.cjs"
		},
		"./server": {
			"types": "./dist/server/index.d.cts",
			"import": "./dist/server/index.js",
			"require": "./dist/server/index.cjs"
		},
		"./web-client": {
			"types": "./dist/web-client/index.d.cts",
			"import": "./dist/web-client/index.js",
			"require": "./dist/web-client/index.cjs"
		}
	},
	"files": [
		"dist",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"license": "MIT",
	"name": "letsync",
	"peerDependencies": {
		"@electric-sql/pglite": "^0.3.5",
		"drizzle-orm": "^0.44.3",
		"express": "^5.1.0",
		"next": "^15.4.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"private": false,
	"scripts": {
		"build": "zshy && biome check --write ./package.json",
		"check:code": "biome check --changed --diagnostic-level=error",
		"check:types": "tsc --noEmit",
		"check:unused": "knip"
	},
	"trustedDependencies": [
		"oxc-resolver"
	],
	"type": "module",
	"version": "0.0.2",
	"zshy": {
		"bin": "./src/cli/index.ts",
		"exports": {
			"./blunt": "./src/(frameworks)/blunt/index.ts",
			"./device-client": "./src/device-client/index.ts",
			"./mutations": "./src/mutations/index.ts",
			"./next": "./src/(frameworks)/next/index.ts",
			"./react": "./src/(frameworks)/react/index.ts",
			"./server": "./src/server/index.ts",
			"./web-client": "./src/web-client/index.ts"
		}
	}
}

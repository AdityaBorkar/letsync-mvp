{
  "author": "Aditya Borkar <hello@adityab.tech>",
  "bin": "./src/cli/index.js",
  "dependencies": {
    "@aws-sdk/client-iot-data-plane": "^3.962.0",
    "arktype": "^2.1.29",
    "commander": "^14.0.2",
    "jsonwebtoken": "^9.0.3",
    "lodash.merge": "^4.6.2",
    "mqtt": "^5.14.1",
    "ora": "^9.0.0",
    "pg": "^8.16.3",
    "pg-logical-replication": "^2.3.0",
    "postgres": "^3.4.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sqlstring": "^2.3.3",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "^1.3.5",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "knip": "^5.79.0",
    "next": "^16.1.1",
    "typescript": "^5.9.3",
    "zshy": "^0.7.0"
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./src/index.js"
    },
    "./drizzle": {
      "import": "./src/(orm)/drizzle/index.js",
      "types": "./src/(orm)/drizzle/index.js"
    },
    "./mutations": {
      "import": "./src/mutations/index.js",
      "types": "./src/mutations/index.js"
    },
    "./next": {
      "import": "./src/(framework)/next/index.js",
      "types": "./src/(framework)/next/index.js"
    },
    "./react": {
      "import": "./src/(framework)/react/index.js",
      "types": "./src/(framework)/react/index.js"
    },
    "./types": {
      "import": "./src/types/index.js",
      "types": "./src/types/index.js"
    },
    "./ws": {
      "import": "./src/(pubsub)/websockets/index.js",
      "types": "./src/(pubsub)/websockets/index.js"
    }
  },
  "files": ["dist", "src", "LICENSE.md", "package.json", "README.md"],
  "keywords": ["letsync"],
  "license": "MIT",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "name": "letsync",
  "peerDependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1"
  },
  "private": false,
  "publishConfig": {
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "zshy && cp -r src/cli/schemas dist/cli/schemas && biome check --fix ./package.json",
    "check:code": "biome check . --fix --diagnostic-level=error",
    "check:types": "tsc --noEmit",
    "check:unused": "knip",
    "dev": "bun run build && bun link",
    "format": "biome check . --fix --diagnostic-level=error",
    "publish": "npm publish --access public"
  },
  "trustedDependencies": ["oxc-resolver", "sharp"],
  "type": "module",
  "version": "0.0.2",
  "zshy": {
    "bin": "./src/cli/index.js",
    "exports": {
      ".": "./src/index.js",
      "./drizzle": "./src/(orm)/drizzle/index.js",
      "./mutations": "./src/mutations/index.js",
      "./next": "./src/(framework)/next/index.js",
      "./react": "./src/(framework)/react/index.js",
      "./ws": "./src/(pubsub)/websockets/index.js"
    }
  }
}
